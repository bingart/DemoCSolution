{
	"manifest_version": 2,
	"name": "publisher",
	"version": "0.0.1",
	"description": "Publisher",
	"icons": {
		"48": "icon48.png",
		"128": "icon128.png",
		"512": "icon512.png"
	},
	"permissions":[
		"tabs",
		"storage",
		"https://*/*",
		"http://*/*"
	],
	"content_scripts":[{
		"matches":["http://kdslife.com/*","http://bbs.dahe.cn/*","https://*/*","http://*/*"],
		"js":[
			"js/jquery-2.0.0.min.js",
			"js/content_script.js"
		]
	}]
}
