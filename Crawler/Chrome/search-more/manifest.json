{
	"manifest_version": 2,
	"name": "search more",
	"version": "0.0.1",
	"description": "Loading next 2,3,4,5 pages automatically within first Bing search page, saving a little clicking time",
	"icons": {
		"48": "icon48.png",
		"128": "icon128.png",
		"512": "icon512.png"
	},
	"permissions":[
		"tabs",
		"https://*/*",
		"http://*/*"
	],
	"content_scripts":[{
		"matches":["https://cn.bing.com/*"],
		"js":["jquery-2.0.0.min.js", "content_script.js"]
	}]
}
